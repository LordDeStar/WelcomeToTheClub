<Page x:Class="WelcomeToTheClub.Views.Pages.RegistartionCompanyPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WelcomeToTheClub.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegistartionCompanyPage">

    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBox x:Name="company"/>
            <Button x:Name="submit" Click="submit_Click" Height="20"/>
        </StackPanel>
    </Grid>
</Page>
